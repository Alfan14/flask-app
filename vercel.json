{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "DATABASE_URL": "mysql://root:password@localhost:3306/mydatabase"
  }
}
